#include<stdio.h>
int fun(int n, int *pla, int *plb, int *plc){
    for(int i=0;i<n;i++){
        plb++;
    }
    for(int i=0;i<n;i++){
        plb--;
        *plc= *pla * *plb;
        pla++;
        plc++;
    }
}
int main(){
    int n;
    scanf("%i", &n);
    int la[n], lb[n], lc[n];
    int *pla=la;
    int *plb=lb;
    int *plc=lc;
    for(int i=0;i<n;i++){
        scanf("%i", &la[i]);
    }
    for(int i=0;i<n;i++){
        scanf("%i", &lb[i]);
    }
    fun(n,pla,plb,plc);
    for(int i=1;i<n;i++){
        printf("%i ", *plc);
        plc++;
    }
    printf("%i", *plc);
}
