#include<stdio.h>
#include<string.h>
typedef struct{
    char nome[50];
    float peso;
    float altura;
    float imc;
}Pessoa;

//inicia dados
void inicia (Pessoa *pessoa, int casos){
    for(int i=0;i<casos; i++){
        strcpy(pessoa[i].nome, "NULL");
        pessoa[i].peso=0;
        pessoa[i].altura=0;
        pessoa[i].imc=0;
    }
}

//cadastra
void cadastra(Pessoa *pessoa, int casos){
    for(int i=0;i<casos;i++){
        scanf("%[^\n]s",pessoa[i].nome);
        
        scanf("%f %f", &pessoa[i].peso, &pessoa[i].altura);
        getchar();
    }
}

//imc
void imc(Pessoa *pessoa, int casos){
    for(int i=0;i<casos;i++){
        pessoa[i].imc = pessoa[i].peso/(pessoa[i].altura*pessoa[i].altura);
    }   
}

//imprime
void imprime(Pessoa *pessoa, int casos){
    for(int i=0;i< casos; i++){
        printf("%.2f ", pessoa[i].imc);
    }
}


int main(){
    int casos;
    
    scanf("%i", &casos);
    getchar();
    
    Pessoa pessoa[casos];
    inicia(pessoa,casos);
    cadastra(pessoa,casos);
    imc(pessoa,casos);
    imprime(pessoa, casos);
    
}
