#include<stdio.h>
#include<string.h>
typedef struct{
    char nome[50];
    float codigo;
    float preco;
}Produto;

//inicia dados
void inicia (Produto *produto, int casos){
    for(int i=0;i<casos; i++){
        strcpy(produto[i].nome, "NULL");
        produto[i].codigo=0;
        produto[i].preco=0;
    }
}

//cadastra
void cadastra(Produto *produto, int casos){
    for(int i=0;i<casos;i++){
        scanf("%[^\n]s",produto[i].nome);
        
        scanf("%f %f", &produto[i].codigo, &produto[i].preco);
        getchar();
    }
}

//procura
void procura(Produto *produto, int casos){
    char procura[50];
    scanf("%[^\n]", procura);
    for(int i=0;i<casos;i++){
        if(strcmp(procura,produto[i].nome)==0){
            printf("%i %s ", i,procura);
            printf("%.0f, %.2f", produto[i].codigo, produto[i].preco);
            break;
        }
        else if(i== casos -1){
            printf("Produto nao encontrado. \n");
        }
    }   
}

int main(){
    int casos;
    
    scanf("%i", &casos);
    getchar();
    
  Produto produto[casos];
  inicia(produto,casos);
  cadastra(produto,casos);
  procura(produto,casos);
}

