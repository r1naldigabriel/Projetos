#include<stdio.h>
int main(){
    int d,m,a;
    
    scanf("%i %i %i",&d,&m,&a);
    
    if(m>12){
        printf("DATA INVALIDA\n");
    }
    else{
        if(a<1900 || a>2036){
            printf("DATA FORA DO INTERVALO ESTIPULADO\n");
        }
        else{
            if(d>31){
                printf("DATA INVALIDA\n");
            }
            else{
                if(a%4==0 && a%100!=0 && a%400==0){
                    if((m==2 || m ==4 || m ==6 || m ==9 || m==11)){
                        if((m==2 && d>29)){
                            printf("DATA INVALIDA\n");
                        }
                        else{
                            if(d>30){
                                printf("DATA INVALIDA\n");
                            }
                            else{
                                printf("%i/%i/%i\n",d,m,a);
                                
                            }
                        }
                    }
                    if(m==1 || m==3 || m==5 || m == 7|| m==8 || m==10 || m==12){
                        if(d>31){
                            printf("DATA INVALIDA\n");
                        }
                        else{
                            printf("%i/%i/%i\n",d,m,a);
                        }
                    }
                }
                else{
                    if(m==2 || m==4 || m==6 || m==9 || m==11){
                        if(m==2 && d>28){
                            printf("DATA INVALIDA\n");
                        }
                        else{
                            if(d>30){
                                printf("DATA INVALIDA\n");
                            }
                            else{
                                printf("%i/%i/%i\n",d,m,a);
                            }
                        }
                    }
                    if(m==1 || m==3 || m==5 || m==7 || m==8 || m==10 || m==12){
                        if(d>31){
                            printf("DATA INVALIDA\n");
                        }
                        else{
                            printf("%i/%i/%i\n",d,m,a);
                        }
                    }
                }
                
            }
        }
    }
}
