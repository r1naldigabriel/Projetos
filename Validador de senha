#include<stdio.h>
int main(){
    int senha;
    char cat;
    scanf("%i %c",&senha,&cat);
    if(senha/100000==0){
        printf("SENHA INVALIDA: %i - MINIMO 6 DIGITOS\n",senha);
    }
    else{
        if(senha>1000000000){
            printf("ACESSO NEGADO\n");
        }
        else{
            if(cat=='A'){
                if(senha==321456){
                    printf("ACESSO PERMITIDO A CATEGORIA A\n");
                }
                else{
                    printf("ACESSO NEGADO\n");
                }
            }
            if(cat=='G'){
                if(senha==9510753){
                    printf("ACESSO PERMITIDO A CATEGORIA G\n");
                }
                else{
                    printf("ACESSO NEGADO\n");
                }
            }
            if(cat=='U'){
                if(senha==78955632){
                    printf("ACESSO PERMITIDO A CATEGORIA U\n");
                }
                else{
                    printf("ACESSO NEGADO\n");
                }
            }
            if(cat!='U' && cat!='A' && cat!='G'){
                printf("CATEGORIA INVALIDA\n");
            }
        }
    }
}
